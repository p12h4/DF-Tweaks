{
  "format_version": "1.10.0",
  "minecraft:attachable": {
    "description": {
      "identifier": "dftweaks:pumpkin_worn.entity",
      "item": {
        "minecraft:pumpkin": "q.is_owner_identifier_any('minecraft:drowned','minecraft:evocation_illager','minecraft:husk','minecraft:piglin','minecraft:piglin_brute','minecraft:pillager','minecraft:player','minecraft:skeleton','minecraft:stray','minecraft:vindicator','minecraft:wither_skeleton','minecraft:zombie','minecraft:zombie_pigman','minecraft:zombie_villager_v2') == 1.0"
      },
      "materials": {
        "default": "armor",
        "enchanted": "armor_enchanted"
      },
      "textures": {
        "default": "textures/models/armor/block/pumpkin",
        "enchanted": "textures/misc/enchanted_item_glint"
      },
      "geometry": {
        "default": "geometry.pumpkin_worn"
      },
      "scripts": {
        "parent_setup": "v.helmet_layer_visible = 0.0;"
      },
      "render_controllers": [
        {
          "controller.render.block_worn.disabled": "c.is_first_person || !q.is_item_name_any('slot.armor.head', 'minecraft:pumpkin')"
        },
        {
          "controller.render.block_worn.enabled": "!c.is_first_person && q.is_item_name_any('slot.armor.head', 'minecraft:pumpkin')"
        }
      ]
    }
  }
}